{
  "node-env": "development",
  "frontend-hostname": {
    "dev": "http://localhost:4200",
    "prod": "http://localhost:4000"
  },

  "opal-api": {
    "url": "http://localhost:4550",
    "opal-fines-service": "http://localhost:4550"
  },
  "features": {
    "redis": {
      "enabled": false
    },
    "sso": {
      "enabled": false
    },
    "helmet": {
      "enabled": false
    },
    "launch-darkly": {
      "enabled": true,
      "stream": true
    },
    "app-insights": {
      "enabled": true
    }
  },
  "session": {
    "prefix": "opal-session",
    "ttlInSeconds": 1200,
    "maxAge": null,
    "secure": false,
    "sameSite": "lax",
    "domain": ""
  },
  "csrf": {
    "cookieName": "XSRF-TOKEN",
    "secure": false,
    "sameSite": "strict"
  },
  "secrets": {
    "opal": {
      "redis-connection-string": "redis://localhost:6379",
      "opal-frontend-session-secret": "supersecret",
      "opal-frontend-cookie-secret": "supersecret",
      "opal-frontend-csrf-secret": "supersecret",
      "app-insights-connection-string": "InstrumentationKey=95778809-169a-413e-a613-a49138ab81b6;IngestionEndpoint=https://uksouth-0.in.applicationinsights.azure.com/;LiveEndpoint=https://uksouth.livediagnostics.monitor.azure.com/;ApplicationId=ce3d4581-7658-48ac-94f4-5029368a83e8"
    }
  },
  "expiry": {
    "testMode": false,
    "default": {
      "expiryTimeInMilliseconds": 1800000,
      "warningThresholdInMilliseconds": 1800000
    },
    "test": {
      "expiryTimeInMilliseconds": 300000,
      "warningThresholdInMilliseconds": 300000
    }
  },
  "application-insights": {
    "cloudRoleName": "opal-frontend"
  }
}
