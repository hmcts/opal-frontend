{
  "node-env": "NODE_ENV",
  "opal-api": {
    "url": "OPAL_API_URL"
  },
  "frontend-hostname": {
    "prod": "FRONTEND_HOSTNAME"
  },
  "features": {
    "redis": {
      "enabled": {
        "__name": "FEATURES_REDIS_ENABLED",
        "__format": "json"
      }
    },
    "sso": {
      "enabled": {
        "__name": "FEATURES_SSO_ENABLED",
        "__format": "json"
      }
    }
  },
  "session": {
    "secure": { "__name": "SESSION_SECURE_ENABLED", "__format": "json" }
  },
  "secrets": {
    "opal": {
      "AppInsightsInstrumentationKey": "APP_INSIGHTS_KEY",
      "redis-connection-string": "REDIS_CONNECTION_STRING"
    }
  }
}
